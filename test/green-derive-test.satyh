@require: stdja
@require: math
@import: ../src/green-derive

module GreenDeriveTest : sig
    val test1 : math
    val test2 : math
end = struct
    open GreenDerive

    let test1 = ${
        \proven!(
            assume(${A})
        )
    }

    let test2 = ${
        \proven!(
            deriveBy({${\wedge}I})
            (${A \wedge B})
            [
                assume(${A});
                assume(${B});
            ]
        )
    }
end